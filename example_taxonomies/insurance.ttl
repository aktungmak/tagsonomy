user:tagsonomy.insurance_demo.auto_policies a uc:Table ;
    uc:conceptAssignment :AutoInsurance ;
    uc:name "tagsonomy.insurance_demo.auto_policies" .

user:tagsonomy.insurance_demo.health_plans a uc:Table ;
    uc:conceptAssignment :HealthInsurance ;
    uc:name "tagsonomy.insurance_demo.health_plans" .

user:tagsonomy.insurance_demo.legacy_claims_history a uc:Table ;
    uc:conceptAssignment :Claim ;
    uc:name "tagsonomy.insurance_demo.legacy_claims_history" .

user:tagsonomy.insurance_demo.legacy_claims_history.clm_num a uc:Column ;
    uc:name "tagsonomy.insurance_demo.legacy_claims_history.clm_num" ;
    uc:propertyAssignment :claimNumber .

user:tagsonomy.insurance_demo.legacy_policy_file a uc:Table ;
    uc:conceptAssignment :Policy ;
    uc:name "tagsonomy.insurance_demo.legacy_policy_file" .

user:tagsonomy.insurance_demo.medical_claims a uc:Table ;
    uc:conceptAssignment :MedicalClaim ;
    uc:name "tagsonomy.insurance_demo.medical_claims" .

user:tagsonomy.insurance_demo.property_claims a uc:Table ;
    uc:conceptAssignment :PropertyClaim ;
    uc:name "tagsonomy.insurance_demo.property_claims" .

user:tagsonomy.insurance_demo.property_claims.claim_ref a uc:Column ;
    uc:name "tagsonomy.insurance_demo.property_claims.claim_ref" ;
    uc:propertyAssignment :claimNumber .

:claimNumber a rdf:Property ;
    rdfs:label "Claim Number" ;
    rdfs:domain :Claim .

